{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es2020": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "ignorePatterns": ["node_modules/", "dist/", "build/", "coverage/"]
}