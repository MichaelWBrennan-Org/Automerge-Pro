    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^8.1.0",
    "@octokit/auth-app": "^8.0.2",
    "@octokit/graphql": "^7.0.2",
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks": "^12.0.8",
    "@prisma/client": "^5.5.2",
    "@types/js-yaml": "^4.0.9",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.14.0",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
